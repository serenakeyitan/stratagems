{
  "name": "stratagems-indexer",
  "version": "0.1.0",
  "description": "stratagems indexer function to generate the state of all the registry messages",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "ethereum-indexer-js-processor": "^0.6.15",
    "named-logs": "^0.2.2",
    "stratagems-common": "workspace:*"
  },
  "devDependencies": {
    "ethereum-indexer-server": "^0.6.16",
    "ldenv": "^0.3.7",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "eis": "eis",
    "serve": "ldenv eis run -n @@RPC_URL_:MODE,RPC_URL -p ./dist/index.cjs --disable-cache --deployments ../contracts/deployments/@@MODE @@",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "echo 'waiting for src/contracts.ts...'; wait-on src/contracts.ts && tsup src/index.ts --dts --format esm,cjs --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
